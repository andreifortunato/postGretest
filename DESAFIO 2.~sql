CREATE TABLE TEMPRESA
( idADMINB SERIAL INTEGER NOT NULL,
sSETOR     SERIAL INTEGER NOT NULL,
sNUCLEO    SERIAL INTEGER NOT NULL,
sdEQUIPE   SERIAL INTEGER NOT NULL,
sdFISCAL   SERIAL INTEGER NOT NULL,
         PRIMARY KEY(idADMINB),
         PRIMARY KEY(sSETOR),
         PRIMARY KEY(sNUCLEO),
         PRIMARY KEY(sEQUIPE),
         PRIMARY KEY(sFISCAL),

                    INSERT INTO TEMPRESA VALUES(010,011,015,018,019);

                     CREATE TABLE FUNCIONARIO(
                     CODIGO INTEGER NULL PRIMARY KEY,
                     ACCESS_DEFAULT varchar(255) not null,
                     check (ACCESS_DEFAULT IN ('SUPERVISOR','DIRETOR','FISCAL'))
);

INSERT INTO FUNCIONARIO (codigo, cor_favorita) values (010, 'DIRETOR');
INSERT INTO FUNCIONARIO (codigo, cor_favorita) values (011, 'SUPERVISOR');
INSERT INTO FUNCIONARIO (codigo, cor_favorita) values (015, 'SUPERVISOR');
INSERT INTO FUNCIONARIO (codigo, cor_favorita) values (018, 'SUPERVISOR');
INSERT INTO FUNCIONARIO (codigo, cor_favorita) values (019, 'FISCAL');
 );

CREATE TABLE TSETOR
( idSETOR    INTEGER NOT NULL  ,
  sSETOR     INTEGER NOT NULL REFERENCES TEMPRESA(sSETOR) ON DELETE CASCADE,
  sNOME      VARCHAR(30) NOT NULL,
  sTITULO    VARCHAR(10) NOT NULL,
  sCABECALHO VARCHAR(50) NOT NULL ,
             CONSTRAINT PK_ACESSO  PRIMARY KEY(idSETOR),
             CONSTRAINT PK_ACESSO  PRIMARY KEY(sSETOR),

INSERT INTO TSETOR VALUES(011,'supervisor 1','titulo 1','cabeçalho 1');
INSERT INTO TSETOR VALUES(012,'nome 2','titulo 2','cabeçalho 2');
INSERT INTO TSETOR VALUES(013,'nome 3','titulo 3','cabeçalho 3');
INSERT INTO TSETOR VALUES(014,'nome 4','titulo 4','cabeçalho 4');

);

CREATE TABLE TNUCLEO
( idNUCLEO        INTEGER NOT NULL,
  sNUCLEO         INTEGER NOT NULL REFERENCES TEMPRESA(sNUCLEO) ON DELETE CASCADE,
  nNOME           VARCHAR(20) NOT NULL ,
  nESTADO         INTEGER NOT NULL,
  nBAIRRO         VARCHAR(30) NOT NULL ,
  nLOGRADOURO     VARCHAR(30),
               CONSTRAINT PK_ACESSON PRIMARY KEY(idNUCLEO),
               CONSTRAINT PK_ACESSON FOREIGN KEY(sNUCLEO);

INSERT INTO TNUCLEO VALUES(015,'supervisor 4','estado 1','bairro 1','logradouro 1');
INSERT INTO TNUCLEO VALUES(016,'nome 5','estado 2','bairro 2','logradouro 2');
INSERT INTO TNUCLEO VALUES(017,'nome 6','estado 3','bairro 3','logradouro 3');
INSERT INTO TNUCLEO VALUES(018,'nome 7','estado 4','bairro 4','logradouro 4');

);


CREATE TABLE TEQUIPE
( idEQUIPE      INTEGER NOT NULL,
  sEQUIPE       INTEGER NOT NULL REFERENCES TEMPRESA(sEQUIPE) ON DELETE CASCADE,
  eNOME         VARCHAR(20) NOT NULL,
  idFISCAL      INTEGER,
           CONSTRAINT PK_ACESSOE PRIMARY KEY(idEQUIPE)
           CONSTRAINT PK_ACESSOE FOREIGN KEY(sEQUIPE)
           CONSTRAINT PK_ACESSOE PRIMARY KEY(idFISCAL)

